{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "dml",
      "camelcase": "Dml",
      "title": "dml",
      "scope": "source.dml",
      "file-types": [
        "dml"
      ],
      "injection-regex": "^dml$",
      "class-name": "TreeSitterDml"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Device Modeling Language (DML) is a domain-specific programming language for developing device models to be simulated with Simics",
    "authors": [
      {
        "name": "wangli-ustc",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/wangli-ustc/tree-sitter-dml",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}